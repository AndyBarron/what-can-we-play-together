
exports = module.exports =

  strcmp: (a, b) =>
    if a < b then -1 else if a > b then 1 else 0

  strcmpCaseInsensitive: (a, b) =>
    exports.strcmp a.toLowerCase(), b.toLowerCase()

  waitSeconds: (seconds) =>
    new Promise (resolve, reject) =>
      setTimeout (=> do resolve), seconds * 1000
