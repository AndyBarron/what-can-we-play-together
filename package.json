{
  "name": "common-games",
  "version": "0.0.1",
  "description": "What Steam games can we play together?",
  "main": "src/node_modules/server/index.js",
  "scripts": {
    "build": "npm-run-all --parallel \"build:*\"",
    "build-prod": "npm-run-all --parallel \"build-prod:*\"",
    "build-prod:client": "cross-env NODE_ENV=production npm-run-all build:client",
    "build:client": "webpack",
    "heroku-postbuild": "npm-run-all build-prod",
    "run:server": "node src/node_modules/server/index.js",
    "scrape": "node src/node_modules/scripts/scrape.js",
    "start": "npm-run-all run:server",
    "watch": "npm-run-all --parallel \"watch:*\"",
    "watch:client": "webpack-dev-server --hot",
    "watch:server": "nodemon ."
  },
  "author": "Andy Barron <AndrewLBarron@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "0.16.2",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "babili-webpack-plugin": "0.1.2",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "2.1.2",
    "html-webpack-plugin": "2.29.0",
    "mkdir-recursive": "0.3.0",
    "nodemon": "1.11.0",
    "normalize.css": "7.0.0",
    "npm-run-all": "4.0.2",
    "openid": "2.0.6",
    "optimize-css-assets-webpack-plugin": "2.0.0",
    "query-string": "4.3.4",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router-dom": "4.1.1",
    "style-loader": "0.18.2",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.4.5"
  },
  "engines": {
    "node": "8.1.3",
    "npm": "5.1.0"
  }
}
